<testDef>

  <info>
This script tests that user can ssh from all nodes to oscar_server

Desired result : PASS
</info>

  <shortDescription>Can user ssh to head from all nodes</shortDescription>

  <test type="script">
  
    <command interpreter="/bin/bash" wdir="/tmp" uname="oscartst">
SOME_NODES_FAILED_SSH=0
for node in $(/usr/lib/oscar/testing/wizard_tests/helpers/oscar_nodes.sh)
do
    if ! ssh $node "ssh -o 'StrictHostKeyChecking no' oscar_server hostname > /dev/null 2>&amp;1"
    then
        SOME_NODES_FAILED=1
        echo "failed to ssh from $node to oscar_server"
    fi
done
exit $SOME_NODES_FAILED_SSH
    </command>

    <output name="stdout" format="literal"></output>
    <output name="stderr" format="literal"></output>
    <output name="status" format="literal">0</output>

  </test>

</testDef>

