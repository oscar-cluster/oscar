<?xml version="1.0" encoding="utf-8"?>
<testDef>

	<info> 
		Tests the unselect_opkg function.
	</info>
	
	<test type="script">
	
		<command interpreter="perl">
#!/usr/bin/perl -w

use strict;
use lib "$ENV{OSCAR_HOME}/lib/OSCAR";
use psm;
use Data::Dumper;

print psm::select_opkg("temp");
print Dumper(psm::show_list());
print psm::unselect_opkg("temp");
print Dumper(psm::show_list());
		</command>
		
		<output name="stdout" format="literal">OK$VAR1 = 'temp';
OK</output>
		<output name="stderr" format="literal"></output>
		<output name="status" format="literal">0</output>
	</test>
</testDef>
