BEGIN {
    if (defined $ENV{OSCAR_HOME}) {
        unshift @INC, "$ENV{OSCAR_HOME}/lib";
    }
}

use strict;
use OSCAR::SystemInfo;
use Carp;

my $info = OSCAR::SystemInfo->new();
$info->proc_info ();
$info->cache_info ();
