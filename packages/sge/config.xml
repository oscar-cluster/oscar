    </versionEntry>
    <versionEntry version="0:6.0u8.0-6">
      <changelogEntry authorName="Erich Focht" date="2007-10-05T16:54:39+02:00">
        <item>converted to new opkgc format</item>
      </changelogEntry>
    </versionEntry>
  </changelog>
</oscar>
