<testDef>

  <info>
This script tests that the workq partition is defined

Desired result : PASS
</info>

  <shortDescription>Tests workq existence</shortDescription>

  <test type="script">
  
    <command interpreter="/bin/bash" wdir="/tmp" uname="oscartst">
if test "`scontrol show partitions|grep '^PartitionName=workq$'|wc -l`" -eq 1
then
	exit 0
else
	echo "ERROR: workq partition not found. Please check with 'scontrol show partitions'."
	exit 1
fi
    </command>

    <output name="stdout" format="regexp"></output>
    <output name="stderr" format="literal"></output>
    <output name="status" format="literal">0</output>

  </test>

</testDef>

