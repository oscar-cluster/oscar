# $Id: setup,v 1.5 2003/01/21 21:55:47 jsquyres Exp $
#

installable=1

if [ `/bin/uname -m` == 'ia64' ]; then
  installable=0
  echo "HDF5 will not be installed due to ia64 incompatibility"
else
  echo "HDF5 will be installed"
fi

perl -pi.bak -e "s/installable\>./installable>$installable/g" \
$OSCAR_PACKAGE_HOME/config.xml
