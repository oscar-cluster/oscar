<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE package SYSTEM "../package.dtd">
<oscar>
  <name>networking</name>

  <version>
    <major>1</major>
    <minor>0</minor>
    <subversion>0</subversion>
    <release>1</release>
    <epoch>1</epoch>
  </version>

  <class>included</class>
  <installable>1</installable>

  <summary>Caching Nameserver to serve the cluster</summary>
  <license>GPL</license>
  <group>Applications/System</group>
  <packager>
    <name>Jason Brechin</name>
    <email>brechin@ncsa.uiuc.edu</email>
  </packager>

  <description>This package contains everything needed to set up your
    distribution's caching nameserver on the OSCAR server.</description>

  <rpmlist>
    <filter group="oscar_server"/>
      <rpm>caching-nameserver</rpm>
      <rpm>bind</rpm>
      <rpm>bind-utils</rpm>
  </rpmlist>

</oscar>
