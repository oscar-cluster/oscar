<?xml version="1.0" encoding="ISO-8859-1"?>
<oscar>
  <name>networking</name>
  <class>included</class>
  <summary>Caching Nameserver to serve the cluster</summary>
  <description>This package contains everything needed to set up your
    distribution's caching nameserver on the OSCAR server.</description>
  <license>GPL</license>
  <group>Applications/System</group>
  <authors>
    <author cat="maintainer">
      <name>Jason Brechin</name>
      <email>brechin@ncsa.uiuc.edu</email>
    </author>
    <author cat="uploader">
      <name>Jean Parpaillon</name>
      <email>jean.parpaillon@irisa.fr</email>
    </author>
  </authors>
  <filters>
    <dist>rhel</dist>
    <dist>fc</dist>
    <dist>mdv</dist>
  </filters>
  <serverDeps>
    <requires>
      <filters>
	<dist>rhel</dist>
	<dist>fc</dist>
	<dist>mdv</dist>
      </filters>      
      <pkg>caching-nameserver</pkg>
    </requires>
  </serverDeps>
  <clientDeps/>
  <apiDeps/>
  <changelog>
    <versionEntry version="1.0.1-1">
      <changelogEntry authorName="Olivier Lahaye" date="2013-10-09T12:00:42+01:00">
        <item>Added "fc" as supported distro</item>
      </changelogEntry>
    </versionEntry>
    <versionEntry version="1.0.0-2">
      <changelogEntry authorName="Jean Parpaillon" date="2007-08-07T14:42:00+01:00">
        <item>Conversion to opkg format v6.0</item>
      </changelogEntry>
    </versionEntry>
    <versionEntry version="1.0.0-1">
      <changelogEntry authorName="Jason Brechin" date="2007-08-07T14:41:38+02:00">
        <item>Original version</item>
      </changelogEntry>
    </versionEntry>
  </changelog>
</oscar>
