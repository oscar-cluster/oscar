cd $HOME
clients=`echo $@ | wc -w`

$HOME/pbs_test $clients 1 $HOME/pvm/pbs_script.pvm "SUCCESSFUL" \
    $HOME/pvm/pvmtext 3 "PVM (via PBS)"

# All done

exit 0
